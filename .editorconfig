# Remove the line below if you want to inherit .editorconfig settings from higher directories
root = false

[*.csproj]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
end_of_line = crlf

[*.json]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
end_of_line = crlf

[*.yml]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
end_of_line = crlf

[*.cs]
charset = utf-8
tab_width = 4
indent_size = 4
end_of_line = crlf